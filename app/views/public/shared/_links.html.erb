<%- if controller_name != 'sessions' %>
  <%= link_to "登録済みの方ははこちら！", new_session_path(resource_name), class: "btn btn-outline-secondary btn-sm" %><br />
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "新規ユーザー登録はこちら！", new_registration_path(resource_name), class: "btn btn-outline-secondary btn-sm" %><br />
<% end %>


